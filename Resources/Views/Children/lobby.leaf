#set("content") {
<h3>Lobby</h3>

<div>
	<div>
        <p>Room code: #(lobby.sessionID)</p>
	</div>
    #for(state in lobby.states){
        <div styles="height:60px;">
            <p>#(state.user)</p>
			<p>#(state.readyState)</p>
        </div>
    }
</div>

<div>
	<button type="button" onClick="ready()">Ready up</button>
</div>
}

#embed("authBase")
