
#set("content") {
<h3>Lobby</h3>

#get(countdown)

<div>
	<div>
        <p>Room code: #(sessionID)</p>
	</div>
    #for(state in states){
        <div styles="height:60px;" class="lobbyReady">
            <p>#(state.user)</p>
			<p>#(state.readyState)</p>
        </div>
    }
</div>

<div>
	<button type="button" class="btn btn-primary" onClick="ready()">Ready up</button>
</div>
}



#embed("authBase")
